{
    "parser": "@typescript-eslint/parser",
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended",
		"prettier"
	],
    "plugins": ["import", "node", "prettier", "@typescript-eslint"],
    "parserOptions": {
        "ecmaVersion": 11
    },
	"rules": {
		"semi": [2, "always"],
		"quotes": [1, "double"]
	}
}
